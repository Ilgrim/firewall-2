{
    "name": "middlewares/firewall",
    "type": "library",
    "description": "Middleware to provide IP filtering",
    "license": "MIT",
    "keywords": [
        "psr-7",
        "psr-15",
        "middleware",
        "server",
        "http",
        "firewall",
        "security",
        "ip",
        "filter"
    ],
    "homepage": "https://github.com/middlewares/firewall",
    "support": {
        "issues": "https://github.com/middlewares/firewall/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "http-interop/http-middleware": "^0.4",
        "m6web/firewall": "^1.0",
        "middlewares/utils": "~0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5 || ^6.0",
        "zendframework/zend-diactoros": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Middlewares\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Middlewares\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpcs"
        ],
        "cs-fix": "php-cs-fixer fix ."
    }
}
